@model List<PostImageViewModel>
@{
    var PostImages = Model;
}
@for (int i = 0; i < Model?.Count(); i++)
{

    var url = PostImages[i].ImageUrl;
    <div class="img-wrapper">
        <a href="@url" target="_blank">@(url?.Substring(url?.LastIndexOf("/")??0))</a> <button type="button" title="@Resource.Delete" class="btn btn-link" onclick="deleteImage('@url',$(this))">x</button>
        @Html.Hidden($"PostImages[{i}].ImageUrl", url)
    </div>

}
